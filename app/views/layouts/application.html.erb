
<!DOCTYPE html>
<html>
<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">

  <title>T.A.P.S</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag "https://js.stripe.com/v1/", "application" %>
  <%= tag :meta, :name => "stripe-key", :content => STRIPE_PUBLIC_KEY %>
  <%#= javascript_include_tag 'gmaps.js' %>
  <%= csrf_meta_tags %>
</head>
<body>
<%=render partial: "layouts/nav_header" %>

<section id="wizard">
<div id="rootwizard">
  <div class="navbar">
    <div class="navbar-inner">
      <div class="container">
  <ul>
    <li><%=link_to 'Student Information','javascript:void(0)',id: "tab1","data-toggle"=> "tab" %></li>
    <li><%=link_to 'Gardian Details',"#",id: "tab2","data-toggle"=> "tab" %></li>
    <li><a href="#tab3" id="tab3" data-toggle="tab">Other Details</a></li>
    <li><a href="#tab4" id="tab4" data-toggle="tab">Readme</a></li>
    <li><a href="#tab5" id="tab5" data-toggle="tab">Payment</a></li>
  </ul>
   </div>
    </div>
  </div>
</div>
</section>

  <% flash.each do |name, msg| %>
    <%= content_tag :div, msg, class: "alert alert-info" %>
  <% end %>


<%= yield %>
</body>
</html>